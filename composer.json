	"keywords"    : [ "wordpress" ],
	"homepage"    : "https://github.com/themehybrid/hybrid-attr",
	"license"     : "GPL-2.0-or-later",
	"authors"     : [
		{
			"name": "Theme Hybrid",
			"email": "themehybrid@gmail.com",
			"homepage": "https://themehybrid.com",
			"role": "Developer"
		}
	],
	"support"     : {
		"issues" : "https://github.com/themehybrid/hybrid-attr/issues",
		"forum"  : "https://themehybrid.com/board/forums/hybrid-core"
	},
	"autoload"    : {
		"psr-4" : {
			"Hybrid\\Attr\\" : "src/"
		},
		"files" : [
			"src/functions-helpers.php"
		]
	},
	"require"     : {
		"php" : ">=5.6",
		"themehybrid/hybrid-contracts" : "^1.0",
		"themehybrid/hybrid-core"      : "^6.0"
	}
}
